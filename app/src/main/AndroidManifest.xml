<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kotlinlogin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".kotlin_normal.LoginActivity"/>
        <activity android:name=".java_mvp.LoginActivity"/>
        <activity android:name=".kotlin_mvp.LoginActivity"/>
        <activity android:name=".java_mvvm.LoginActivity"/>
        <activity android:name=".kotlin_vipe.LoginActivity"/>
        <activity android:name=".kotlin_viper.LoginActivity"/>
    </application>

</manifest>